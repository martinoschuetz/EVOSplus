name                 =       "Mice" 	# Program Name
run                  =       "naca" 	# Experiment Name
sync                 =          no 	# Synchronous Communication
tasks	             =           1	#
isolation	     =           1	#
width                =          10 	# Field Width
height               =           1 	# Field Height
nbhsize              =           1 	# Neighborhood Size
children             =           1 	# Children Per Individual
select               =        plus 	# Selection Scheme
reco_x               =  arithmetic 	# Recombination x[i]
reco_s               =  arithmetic	# Recombination sigma[i]
maxgen               = 5 # Maximum No Of Generations
# maxcalls            = <not set> 	# Maximum No Of Function Calls
bits                 =		 0
mrate		     =       0.002
cprob		     =	       0.5
init_zeroes	     =	       0.5
vars                 =          12 	# No Of Variables
sigmas               =          n 	# No Of Sigmas
tau0                 =        0.1	# Global Sigma Variance
taui                 =        0.3 	# Local Sigma Variance
min_init_x           =        1e3 	# Lower Boundary For Initial x[i]
max_init_x           =        1e3 	# Upper Boundary For Initial x[i]
min_init_s           =        1e1 	# Lower Boundary For Initial sigma[i]
max_init_s           =        1e2 	# Upper Boundary For Initial sigma[i]
smin                 =     1e-200  	# Minimum sigma[i]
smax                 =      1e200  	# Maximum sigma[i]
# stopat              = <not set> 	# Stop when f(x)	is better
# seed                = <not set> 	# Random Seed
dmpint               =          0 	# Dump Interval
logfile             = naca.out
pixmon               =        off 	# Use PixMon
